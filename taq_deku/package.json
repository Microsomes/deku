{
	"name": "@deku/taqueria-plugin-deku",
	"version": "0.12.0",
	"description": "Todo: A plugin for Taqueria providing ipfs publishing and pinning using the Pinata service",
	"keywords": [
		"taqueria",
		"plugin",
		"jest",
		"testing",
		"tdd",
		"ecad",
		"ecadlabs",
		"tezos"
	],
	"targets": {
		"default": {
			"source": "./index.ts",
			"distDir": "./",
			"context": "node",
			"isLibrary": true,
			"outputFormat": "esmodule"
		}
	},
	"scripts": {
