	"author": "ECAD Labs",
	"license": "Apache-2.0",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ecadlabs/taqueria.git",
		"directory": "taqueria-plugin-ipfs-pinata"
	},
	"dependencies": {
		"@taqueria/node-sdk": "^0.12.0"
	},
	"devDependencies": {
		"parcel": "2.6.1",
		"typescript": "4.7.2"
	}
}
